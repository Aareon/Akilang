# Demo of class definitions across function boundaries

class myClass {
    x:i32
    prop:u32
    other:u32
    string:str
}

def fn2(x:myClass, x1:i32, s1:str): myClass{
    x.string=s1
    x.x=x1
    print (x.x)
    print (x.string)
    return x
}

def main(){
    var c1:myClass
    c1 = fn2(c1, 64, "Hi you")
    print (c1.x)
    print (c1.string)
    return 0
}